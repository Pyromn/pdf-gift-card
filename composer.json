{
    "name": "pyromn/pdf-gift-card",
    "description": "Library to generate PDF gift cards",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "keywords": ["gift-card", "pdf"],
    "support": {
        "source": "https://github.com/pyromn/pdf-gift-card"
    },
    "authors": [
        {
            "name": "Roman Vesely",
            "homepage": "https://github.com/pyromn"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-mbstring": "*",
        "setasign/tfpdf": "^1.33"
    },
    "require-dev": {
        "phpunit/phpunit": "8 - 9"
    },
    "autoload": {
        "psr-4": { "Pyromn\\PdfGiftCard\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Pyromn\\PdfGiftCard\\Tests\\": "tests/"
        }
    }
}
